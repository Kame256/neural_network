    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def sigmoid(x):\n",
    "    return 1/(1+np.exp(-x))\n",
    "\n",
    "x=np.arange(-5.0,5.0,0.01)\n",
    "y=sigmoid(x)\n",
    "plt.plot(x,y)\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
  }
